{
	"name": "sachink23/serverless-php",
	"version": "v0.02-alpha",
	"type": "template",
	"description": "Framework based on https://bref.sh/ for building serverless PHP applications on AWS Lambda",
	"keywords": [
		"serverless-php"
	],
	"homepage": "https://serverless-php.sachink.com",
	"minimum-stability": "dev",
	"prefer-stable": true,
	"license": "MIT",
	"autoload": {
		"classmap": [
			"src/classes/",
			"src/interfaces/",
			"src/app/"
		]
	},
	"authors": [
		{
			"name": "Sachin Kekarjawalekar",
			"email": "sachin@kekarjawalekar.com",
			"role": "Developer"
		}
	],
	"require": {
		"php": "^7.3",
		"ext-json": "*",
		"bref/bref": "^0.5.24",
		"mnapoli/silly": "^1.7"
	},
	"require-dev": {
		"phpunit/phpunit": "^9"
	}
}
